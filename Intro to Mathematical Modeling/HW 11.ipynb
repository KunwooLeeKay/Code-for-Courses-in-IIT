   ],
   "source": [
    "def dxdt(x,y):\n",
    "    return x - x*y -3/4*y\n",
    "\n",
    "def dydt(x,y):\n",
    "    return x*y - y -3/4*x\n",
    "\n",
    "x = [1/2]; y = [1]\n",
    "\n",
    "for _ in range(4):\n",
    "    x.append(x[-1] + dxdt(x[-1], y[-1]))\n",
    "    y.append(y[-1] + dydt(x[-1], y[-1]))\n",
    "\n",
    "print(\"x :\", x)\n",
    "print(\"y :\", y)"
   ]
